<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.reportcardapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterStudent" />
        <activity android:name=".Functions" />
        <activity android:name=".ShowStudentDetailsReport" />
        <activity android:name=".ShowSearchStudent" />
        <activity android:name=".UpdateStudent" />
        <activity android:name=".StudentListForSubjects" />
        <activity android:name=".SubjectAndStudent"></activity>
    </application>

</manifest>